<script type="text/javascript" charset="utf-8">
$(document).ready(function() {
	$('#create_room').click(function() {
		$('#create_room_div').css('display', 'none');
	});
	
	$('.edit').each(function() {
		$(this).click(function() {
			$('#create_room_div').css('display', 'none');
		});
	});
});

$("#cancel_room_creation").click(function() {
	$('#form_add_rooms').empty();
	$('#create_room_div').css('display', 'block');
});
</script>
