<%- content_for :head_end do -%>
	<%= stylesheet_link_tag 'events' %>
<%- end -%>
<script type="text/javascript" charset="utf-8">
	<%= render :partial => 'javascripts' %>
</script>
<h1>New event</h1>

<% form_for(@event) do |f| %>
	<%= f.error_messages %>
	<%= render :partial => f %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', events_path %>