<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%=h room.name %></td>
    <td><%= link_to 'Edit', edit_room_path(room) %></td>
    <td><%= link_to 'Destroy', room, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New room', new_campus_room_path(@campus) %>