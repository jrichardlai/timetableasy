$("#new_room_creation").click( function() {
 	$('#form_add_rooms').empty();
	$('#create_room_div').css('display', 'block');
});

$("#edit_period_update").click( function() {
 	$('#form_add_periods').empty();
	$('#create_period_div').css('display', 'block');
});

$("#cancel_room_creation").click(function() {
	$('#form_add_rooms').empty();
	$('#create_room_div').css('display', 'block');
	return false;
});
